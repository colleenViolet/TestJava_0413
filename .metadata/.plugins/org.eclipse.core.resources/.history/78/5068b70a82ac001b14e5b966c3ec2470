		int[] numbers = {3, 8, 9, 4, 2, 1, 7, 5};
		numbers[4]= 6;
		
		for(int i = 0; i < numbers.length; i++) {
			System.out.print(numbers[i] + " ");
		}
		
		for(int i = numbers.length; i > 0; i--) {
			System.out.print(numbers[i] + " ");
		}
		
		for(int i = 0; i < numbers.length; i++) {
			if(i % 2 == 0) {
				System.out.println(numbers[i]);
			}
		}
		
		for(int i = 0; i < numbers.length; i++) {
			if(numbers[i] == 2) {
				System.out.println("2가 들어가 있는 index : " + i);
			}
		}
		
